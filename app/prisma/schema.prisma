generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model BPMHistory {
  id         Int      @id @default(autoincrement())
  value      Int
  created_at DateTime @default(now())
}

model ActivityHistory {
  id         Int      @id @default(autoincrement())
  value      String
  created_at DateTime @default(now())
}
